#include <iostream>
using namespace std;
//define months
string months[] = {"Jan" , "Feb" , "Mar" , "Apr" , "May" , "Jun" , "Jul" , "Aug" , "Sep" , "Oct" , "Nov" , "Dec"};

string ReformatDate(string);

int main() {
  //get the input and define olddate
  string oldDate;
  getline(cin , oldDate);
  string newDate = ReformatDate(oldDate);
  cout<<newDate<<endl;
  return 0;
}
//create function
string ReformatDate(string oldDate){
  int dateLength = oldDate.length();
  string newDate = "";
  for(int i = 4 ; i > 0  ; i--)
    newDate += oldDate[dateLength - i];
  newDate += " - ";
  string month = "";
  month += oldDate[dateLength - 8];
  month += oldDate[dateLength - 7];
  month += oldDate[dateLength - 6];
  for(int i = 0 ; i < 12 ; i++){
    if(months[i] == month){
      if(i < 9){
        newDate += '0';
        newDate += (char)((i + 1) + '0');
      }else
        int a = (i + 1)/10;
        int b = (i + 1)%10;
        newDate += (char)(a + '0');
        newDate += (char)(b + '0');
      }
    }
  }
  newDate += " - ";
  if(dateLength == 13){
    newDate += oldDate[0];
    newDate += oldDate[1];
  }else{
    newDate += '0';
    newDate += oldDate[0];
  }
  return newDate;
}
